{
    "version": 2,
    "env": {
        "NODE_ENV": "production"
    },
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb",
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}